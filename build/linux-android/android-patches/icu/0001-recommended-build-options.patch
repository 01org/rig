From c8f76d62865824a41cdc3577abac9cdb5e8299d1 Mon Sep 17 00:00:00 2001
From: Robert Bragg <robert@linux.intel.com>
Date: Mon, 20 May 2013 13:54:11 +0100
Subject: [PATCH] recommended build options

---
 source/common/unicode/platform.h | 8 ++++----
 source/common/unicode/uversion.h | 2 +-
 2 files changed, 5 insertions(+), 5 deletions(-)

diff --git a/source/common/unicode/platform.h b/source/common/unicode/platform.h
index 26f12f4..6093002 100644
--- a/source/common/unicode/platform.h
+++ b/source/common/unicode/platform.h
@@ -408,7 +408,7 @@
 #endif
 
 /**
- * \def U_HAVE_DEBUG_LOCATION_NEW 
+ * \def U_HAVE_DEBUG_LOCATION_NEW
  * Define this to define the MFC debug version of the operator new.
  *
  * @stable ICU 3.4
@@ -545,7 +545,7 @@
 #elif U_PLATFORM == U_PF_ANDROID || U_PLATFORM_IS_DARWIN_BASED
 #   define U_CHARSET_IS_UTF8 1
 #else
-#   define U_CHARSET_IS_UTF8 0
+#   define U_CHARSET_IS_UTF8 1
 #endif
 
 /** @} */
@@ -707,7 +707,7 @@
 #elif defined(__GNUC__)
 #   define U_EXPORT __attribute__((visibility("default")))
 #elif (defined(__SUNPRO_CC) && __SUNPRO_CC >= 0x550) \
-   || (defined(__SUNPRO_C) && __SUNPRO_C >= 0x550) 
+   || (defined(__SUNPRO_C) && __SUNPRO_C >= 0x550)
 #   define U_EXPORT __global
 /*#elif defined(__HP_aCC) || defined(__HP_cc)
 #   define U_EXPORT __declspec(dllexport)*/
@@ -732,7 +732,7 @@
     /* Windows needs to export/import data. */
 #   define U_IMPORT __declspec(dllimport)
 #else
-#   define U_IMPORT 
+#   define U_IMPORT
 #endif
 
 /**
diff --git a/source/common/unicode/uversion.h b/source/common/unicode/uversion.h
index 74e3091..7129b0f 100644
--- a/source/common/unicode/uversion.h
+++ b/source/common/unicode/uversion.h
@@ -120,7 +120,7 @@ typedef uint8_t UVersionInfo[U_MAX_VERSION_LENGTH];
 #   define U_NAMESPACE_QUALIFIER U_ICU_NAMESPACE::
 
 #   ifndef U_USING_ICU_NAMESPACE
-#       define U_USING_ICU_NAMESPACE 1
+#       define U_USING_ICU_NAMESPACE 0
 #   endif
 #   if U_USING_ICU_NAMESPACE
         U_NAMESPACE_USE
-- 
